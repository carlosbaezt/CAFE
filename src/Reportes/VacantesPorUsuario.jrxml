	<queryString>
		<![CDATA[select usuariovacante.cedula,vacante.cargo,empresa.nombre as nombre_Empresa,ciudad.nombre as nombre_Ciudad

from usuariovacante

inner join vacante on
usuariovacante.id_vacante = vacante.id

inner join empresa on
empresa.nit = vacante.nit

inner join ciudad on
vacante.ciudad = ciudad.ciudad

where usuariovacante.cedula = $P{cedula}]]>
	</queryString>
